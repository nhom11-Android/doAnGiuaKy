<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ThongKe">
    <TextView
        android:id="@+id/vatTuDungNhieuTv_ThongKe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Vật tư vận chuyển nhiều nhất"
        android:textColor="@color/black"
        android:textSize="17dp"
        android:background="@drawable/border"
        android:padding="5dp"
        android:onClick="callThongKeVatTuDungNhieu"/>
    <TextView
        android:id="@+id/vatTuDoanhThuCaoTv_ThongKe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Vật tư có doanh thu cao"
        android:textColor="@color/black"
        android:textSize="17dp"
        android:background="@drawable/border"
        android:padding="5dp"
        android:onClick="callThongKeVatTuDoanhThuCao"/>
    <TextView
        android:id="@+id/congTrinhVatTuTv_ThongKe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Công trình theo loại vật tư"
        android:background="@drawable/border"
        android:textColor="@color/black"
        android:textSize="17dp"
        android:padding="5dp"
        android:onClick="callThongKeCongTrinh"/>
    <TextView
        android:id="@+id/phieuVanChuyenVatTuTv_ThongKe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Phiếu vận chuyển theo vật tư"
        android:background="@drawable/border"
        android:textColor="@color/black"
        android:textSize="17dp"
        android:padding="5dp"
        android:onClick="callThongKePhieuVanChuyen"/>



</LinearLayout>